<%= form_for @user do |f| %>

<%= f.text_field :first_name %>
<%= f.text_field :last_name %>
<%= f.text_field :email %>
<%= f.text_field :phone %>
<%= f.text_field :address_1 %>
<%= f.text_field :address_2 %>
<%= f.text_field :city %>
<%= f.text_field :state %>
<%= f.text_field :zip %>
<%= f.text_field :bio %>

<%= f.submit "Edit Account" %>

<%end%>
